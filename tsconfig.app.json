{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/**/*", //
    "src/**/*.ts",
    "src/**/*.vue",
    "src/**/*.tsx",
    "typings/**/*.d.ts"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "types": ["node"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
